extends ../layout.pug


block styles 
  link(rel="stylesheet", href="/css/user.css")

block content
  

  h1 Lista de Usuários

  .users-container
    each user in userList
      .user-card
        .user-name #{user.nome}
        .user-info Contato: #{user.contato}
        .user-info Endereço: #{user.endereco}
        .user-info Papel: #{user.role}